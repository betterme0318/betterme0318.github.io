{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "jsx": "react",
    "module": "commonjs",
    "target": "es5",
    "allowJs": false,
    "allowUnusedLabels": true,
    "noUnusedLocals": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "declaration": true,
    "baseUrl": ".",
    "outDir": "./build",
    "lib": [
      "ES2015.Promise",
      "DOM",
      "ES2015",
      "es2017"
    ]
  }
}